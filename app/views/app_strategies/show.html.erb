<div id="index-banner" class="parallax-container layout-app-strategies">
  <div class="section no-pad-bot">
    <% case params[:id] %>
    <% when '1' %>
      <%= render 'pairs' %>
    <% when '2' %>
      <%= render 'with' %>
    <% when '3' %>
      <%= render 'tapple' %>
    <% when '4' %>
      <%= render 'tinder' %>
    <% end %>
  </div>
</div>